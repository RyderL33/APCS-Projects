    public class Rectangle
    {
    private int x, y, width, height; //for the sake of this class and for AP exam, always initialize the variables with "private"
      public Rectangle(int x, int y, int width, int height)
      {
        this.x = x;
        this.y = y;
        this.width = width;
        this.height = height;
      }

      public void calcArea()
      {
        System.out.println("Area :" + (width * height));
      }

    

		
  
    

  }
